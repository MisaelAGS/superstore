   "source": [
    "import pandas as pd\n",
    "df = pd.read_csv('data/superstore.csv', encoding='ISO-8859-1')\n",
    "df.groupby('City', as_index=False)['Profit'].sum().rename(columns={\n",
    "    \"City\": \"Cidade\",\n",
    "    \"Profit\": \"Lucro Total\"\n",
    "}).sort_values(\"Lucro Total\", ascending=False)"
   ]
  }
